{
    "name": "gdelgado/quotes",
    "description": "package for connection to dummyjson api, graphical interface with vuejs",
    "type": "package",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Gdelgado\\Quotes\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Gdelgado\\Quotes\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/",
            "Workbench\\Database\\Factories\\": "workbench/database/factories/",
            "Workbench\\Database\\Seeders\\": "workbench/database/seeders/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gdelgado\\Quotes\\Providers\\QuotesServiceProvider"
            ]
        }
    },
    "authors": [
        {
            "name": "Gustavo",
            "email": "gustavo.a.delgado.o@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "laravel/framework": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "10.x-dev"
    },
    "scripts": {
        "post-autoload-dump": [
            "@clear",
            "@prepare"
        ],
        "clear": "@php vendor/bin/testbench package:purge-skeleton --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": "@php vendor/bin/testbench workbench:build --ansi",
        "serve": [
            "Composer\\Config::disableProcessTimeout",
            "@build",
            "@php vendor/bin/testbench serve --ansi"
        ],
        "test": [
            "@clear",
            "@php vendor/bin/phpunit"
        ]
    }
}